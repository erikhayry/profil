{
	"manifest_version": 2,
	"name": "Profiler",
	"version": "0.1",
	"description": "Profiler f√∂r SVT Play",
	"icons": {
		"48": "icons/logo.png",
		"96": "icons/logo@2x.png"
	},
	"background": {
		"scripts": [
			"dist/background.js"
		]
	},
	"content_scripts": [
		{
			"matches": [
				"https://www.svtplay.se/*"
			],
			"js": [
				"dist/content.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icons/logo.png",
		"default_title": "Profiler",
		"default_popup": "/ui/index.html"
	},
	"options_ui": {
		"page": "/ui/index.html",
		"open_in_tab": true
	},
	"permissions": [
		"storage"
	]
}