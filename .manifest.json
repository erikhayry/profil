{
  "manifest_version": 2,
  "name": "Profiler",
  "version": "0.1",
  "description": "Profiler f√∂r SVT Play",
  "icons": {
    "48": "icons/logo.png",
    "96": "icons/logo@2x.png"
  },
  "background": {
    "scripts": [
      "/node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
      "/node_modules/@sentry/browser/build/bundle.min.js",
      "/node_modules/moment/min/moment.min.js",
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.svtplay.se/*"
      ],
      "js": [
        "/node_modules/webextension-polyfill/dist/browser-polyfill.min.js",
        "/node_modules/@sentry/browser/build/bundle.min.js",
        "index.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_title": "Profiler"
  },
  "options_ui": {
    "page": "/dist/options.html",
    "open_in_tab": true
  },
  "permissions": [
    "history",
    "storage"
  ],
  "web_accessible_resources": [
    "/static/bg-min.png",
    "/dist/landing.html"
  ]
}